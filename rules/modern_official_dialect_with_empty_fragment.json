{
  "title": "Metaschema URI must not include trailing `#` (Draft 2019-09+)",
  "description": "The official dialect URI of 2019-09 and newer versions must not contain the empty fragment (no trailing #).",
  "examples": [
    {
      "after": {
        "$schema": "https://json-schema.org/draft/2019-09/schema",
        "type": "string"
      },
      "before": {
        "$schema": "https://json-schema.org/draft/2019-09/schema#",
        "type": "string"
      },
      "doc": true
    },
    {
      "after": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object"
      },
      "before": {
        "$schema": "https://json-schema.org/draft/2020-12/schema#",
        "type": "object"
      }
    }
  ],
  "autofixable": true,
  "categories": [ "correctness", "style" ],
  "dialects": {
    "2019-09": [ "$schema" ],
    "2020-12": [ "$schema" ]
  },
  "message": "Remove the trailing `#` from the metaschema URI.",
  "references": [
    "https://json-schema.org/draft/2019-09/schema",
    "https://json-schema.org/draft/2020-12/schema"
  ]
}
