{
  "title": "Collapse duplicate schemas in `allOf`",
  "description": "`allOf` with duplicate branches is redundant and can be collapsed.",
  "examples": [
    {
      "after": {
        "allOf": [
          {
            "type": "string"
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "string"
          },
          {
            "type": "string"
          }
        ]
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "readability", "style" ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: Duplicate identical allOf branches",
      "description": "Remove exact duplicates; they add no value.",
      "after": {
        "allOf": [
          {
            "type": "string"
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "string"
          },
          {
            "type": "string"
          }
        ]
      }
    },
    {
      "title": "Wrong: Semantically equivalent branches",
      "description": "Branches that accept the same instances are duplicates.",
      "after": {
        "allOf": [
          {
            "type": "integer"
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "integer"
          },
          {
            "type": "number",
            "multipleOf": 1
          }
        ]
      }
    },
    {
      "title": "Right: Complementary constraints",
      "description": "Distinct constraints that narrow the set together are valid.",
      "after": {
        "allOf": [
          {
            "type": "string"
          },
          {
            "minLength": 5
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "string"
          },
          {
            "minLength": 5
          }
        ]
      }
    }
  ],
  "dialects": {
    "2019-09": [ "allOf" ],
    "2020-12": [ "allOf" ],
    "draft4": [ "allOf" ],
    "draft6": [ "allOf" ],
    "draft7": [ "allOf" ]
  },
  "message": "Remove duplicate branches; keep one instance.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-core.html#name-allof"
  ]
}
