{
  "title": "Collapse duplicate schemas in `allOf`",
  "description": "Setting duplicate subschemas in `allOf` is redundant and only adds unnecessary validation work.",
  "examples": [
    {
      "after": {
        "allOf": [
          {
            "type": "string"
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "string"
          },
          {
            "type": "string"
          }
        ]
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "readability", "style" ],
  "dialects": {
    "2019-09": [ "allOf" ],
    "2020-12": [ "allOf" ],
    "draft4": [ "allOf" ],
    "draft6": [ "allOf" ],
    "draft7": [ "allOf" ]
  },
  "message": "Remove duplicate branches; keep just one instance.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-core.html#name-allof"
  ]
}
