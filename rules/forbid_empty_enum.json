{
  "title": "Disallow empty `enum` arrays",
  "description": "`enum` must contain at least one element. An empty array always fails validation and is likely a mistake.",
  "examples": [
    {
      "after": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "enum": [ "option1", "option2" ]
      },
      "before": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "enum": []
      },
      "doc": true
    }
  ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: Empty enum is not allowed",
      "description": "`enum` must contain at least one value; empty arrays always fail.",
      "before": { "enum": [] },
      "after": { "enum": ["option1"] }
    },
    {
      "title": "Right: Non-empty enum",
      "description": "A populated enum is valid.",
      "before": { "enum": [1,2] },
      "after": { "enum": [1,2] }
    }
  ],
  "autofixable": false,
  "categories": [ "correctness" ],
  "dialects": {
    "2019-09": [ "enum" ],
    "2020-12": [ "enum" ],
    "draft4": [ "enum" ],
    "draft6": [ "enum" ],
    "draft7": [ "enum" ]
  },
  "message": "Remove empty enum or add allowed values.",
  "references": [
    "https://json-schema.org/understanding-json-schema/reference/generic.html#enumerated-values"
  ]
}
