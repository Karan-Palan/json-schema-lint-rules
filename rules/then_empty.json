{
  "title": "`then` is an empty schema",
  "description": "`then` set to the empty schema (`{}`) does not restrict validation and is most likely ineffective.",
  "examples": [
    {
      "after": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        }
      },
      "before": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        },
        "then": {}
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "correctness" ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: then is an empty schema",
      "description": "Empty then adds no constraint; remove or add restrictions.",
      "after": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        }
      },
      "before": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        },
        "then": {}
      }
    },
    {
      "title": "Right: then with constraints",
      "description": "Provide actual constraints in then to make the conditional effective.",
      "after": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        },
        "then": {
          "required": [ "flag" ]
        }
      },
      "before": {
        "if": {
          "properties": {
            "flag": {
              "const": true
            }
          }
        },
        "then": {
          "required": [ "flag" ]
        }
      }
    }
  ],
  "dialects": {
    "2019-09": [ "if", "then", "else" ],
    "2020-12": [ "if", "then", "else" ],
    "draft7": [ "if", "then", "else" ]
  },
  "message": "Remove empty `then` or add restrictions.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-core.html#name-conditionals"
  ]
}
