{
  "title": "`contentSchema` without `contentMediaType` is meaningless",
  "description": "The `contentSchema` keyword has no effect unless `contentMediaType` is also present.",
  "examples": [
    {
      "after": {
        "type": "string",
        "contentMediaType": "application/json",
        "contentSchema": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "$id": "https://example.com/schema",
          "type": "object"
        }
      },
      "before": {
        "type": "string",
        "contentSchema": {
          "type": "object"
        }
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "readability", "style" ],
  "dialects": {
    "2019-09": [ "contentSchema", "contentMediaType" ],
    "2020-12": [ "contentSchema", "contentMediaType" ],
    "draft7": [ "contentSchema", "contentMediaType" ]
  },
  "message": "Add `contentMediaType` or remove `contentSchema`.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-validation.html#name-contentschema"
  ]
}
