{
  "title": "Duplicate values in `required`",
  "description": 	"Setting duplicate values in required` is considered an anti-pattern.",
  "examples": [
    {
      "after": {
        "type": "object",
        "required": [ "id", "name" ]
      },
      "before": {
        "type": "object",
        "required": [ "id", "name", "id" ]
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "correctness" ],
  "dialects": {
    "2019-09": [ "required" ],
    "2020-12": [ "required" ],
    "draft4": [ "required" ],
    "draft6": [ "required" ],
    "draft7": [ "required" ]
  },
  "message": "Remove duplicate values from the `required` array.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-validation.html#name-required"
  ]
}
