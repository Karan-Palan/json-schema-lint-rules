{
  "title": "Remove empty `dependencies` object",
  "description": "`dependencies:{}` never alters validation; it should be dropped.",
  "examples": [
    {
      "after": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object"
      },
      "before": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "dependencies": {}
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "style" ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: dependencies:{} is redundant",
      "description": "An empty dependencies object has no effect.",
      "after": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object"
      },
      "before": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "dependencies": {}
      }
    },
    {
      "title": "Right: Non-empty dependencies is meaningful",
      "description": "Listing property dependencies changes validation and is valid.",
      "after": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "dependencies": {
          "a": [ "b" ]
        }
      },
      "before": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "dependencies": {
          "a": [ "b" ]
        }
      }
    }
  ],
  "dialects": {
    "draft4": [ "dependencies" ],
    "draft6": [ "dependencies" ],
    "draft7": [ "dependencies" ]
  },
  "message": "Delete the empty `dependencies` object.",
  "references": [
    "https://json-schema.org/draft/07/json-schema-validation.html#dependencies"
  ]
}
