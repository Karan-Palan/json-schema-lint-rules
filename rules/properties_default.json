{
  "title": "Remove empty `properties` object",
  "description": "`properties:{}` does not impose any constraints. Drop it to reduce clutter.",
  "examples": [
    {
      "after": {
        "type": "object"
      },
      "before": {
        "type": "object",
        "properties": {}
      },
      "doc": true
    },
    {
      "after": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "object"
          }
        ]
      },
      "before": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "object",
            "properties": {}
          }
        ]
      }
    },
    {
      "after": {
        "allOf": [
          {
            "type": "object"
          }
        ]
      },
      "before": {
        "allOf": [
          {
            "type": "object",
            "properties": {}
          }
        ]
      }
    }
  ],
  "autofixable": true,
  "categories": [ "style" ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: Empty properties on a plain object",
      "description": "An empty properties object has no effect and should be removed to reduce noise.",
      "after": {
        "type": "object"
      },
      "before": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "title": "Wrong: Empty properties inside anyOf branch",
      "description": "Dropping the empty properties keeps the intended alternative intact.",
      "after": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "object"
          }
        ]
      },
      "before": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "object",
            "properties": {}
          }
        ]
      }
    },
    {
      "title": "Right: Non-empty properties is fine",
      "description": "A populated properties object is meaningful and should remain.",
      "after": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      },
      "before": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      }
    }
  ],
  "dialects": {
    "2019-09": [ "properties" ],
    "2020-12": [ "properties" ],
    "draft4": [ "properties" ],
    "draft6": [ "properties" ],
    "draft7": [ "properties" ]
  },
  "message": "Delete the empty `properties` object.",
  "references": [
    "https://json-schema.org/understanding-json-schema/reference/object.html#properties"
  ]
}
