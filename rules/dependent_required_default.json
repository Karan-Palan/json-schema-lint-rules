{
  "title": "Remove empty `dependentRequired` object",
  "description": "`dependentRequired:{}` has no effect and should be removed.",
  "examples": [
    {
      "after": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object"
      },
      "before": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "dependentRequired": {}
      },
      "doc": true
    }
  ],
  "autofixable": true,
  "categories": [ "style" ],
  "comprehensiveExamples": [
    {
      "title": "Wrong: dependentRequired:{} is redundant",
      "description": "An empty object under dependentRequired has no effect.",
      "after": {
        "type": "object"
      },
      "before": {
        "type": "object",
        "dependentRequired": {}
      }
    },
    {
      "title": "Right: Non-empty dependentRequired",
      "description": "Specifying required properties conditionally is valid.",
      "after": {
        "type": "object",
        "dependentRequired": {
          "creditCard": [ "billingAddress" ]
        }
      },
      "before": {
        "type": "object",
        "dependentRequired": {
          "creditCard": [ "billingAddress" ]
        }
      }
    }
  ],
  "dialects": {
    "2019-09": [ "dependentRequired" ],
    "2020-12": [ "dependentRequired" ]
  },
  "message": "Delete the empty `dependentRequired` object.",
  "references": [
    "https://json-schema.org/draft/2020-12/json-schema-validation.html#name-dependentrequired"
  ]
}
